{
  "include": ["src"],
  "exclude": [
    "**/__pycache__",
    "**/node_modules",
    ".venv",
    "**/.venv"
  ],
  "venvPath": ".",
  "venv": ".venv",
  "pythonVersion": "3.10",
  "pythonPlatform": "Linux",
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "stubPath": "src"
}
