{
  "name": "agent-bench",
  "version": "0.2.0",
  "description": "An open-source benchmark for evaluating AI coding agents on real-world engineering tasks",
  "type": "module",
  "bin": {
    "agent-bench": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "start": "bun run dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@opencode-ai/sdk": "1.2.6",
    "commander": "12.1.0",
    "js-yaml": "4.1.1",
    "zod": "3.25.76",
    "simple-git": "3.31.1",
    "csv-stringify": "6.6.0",
    "chalk": "5.6.2",
    "ora": "8.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "4.0.9",
    "@types/node": "22.19.11",
    "bun-types": "1.3.9",
    "typescript": "5.9.3"
  },
  "keywords": [
    "agent",
    "benchmark",
    "ai",
    "coding",
    "opencode",
    "evaluation"
  ],
  "author": "Agent Bench Contributors",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  }
}
